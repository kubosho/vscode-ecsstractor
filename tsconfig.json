{
  "compilerOptions": {
    "module": "commonjs",
    "target": "esnext",
    "outDir": "out",
    "lib": [
      "esnext"
    ],
    "sourceMap": true,
    "rootDir": "src",
    "strict": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "noUnusedParameters": true,
    "typeRoots": [
      "./node_modules/@types",
      "./typings"
    ],
    "types": []
  },
  "exclude": [
    "node_modules",
    ".vscode-test"
  ]
}
