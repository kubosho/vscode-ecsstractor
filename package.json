{
  "name": "ecsstractor",
  "displayName": "eCSStractor",
  "description": "Extracting selectors from HTML / JSX / TSX and generate CSS file.",
  "version": "2.4.2",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/kubosho/vscode-ecsstractor.git"
  },
  "publisher": "kubosho",
  "engines": {
    "vscode": "^1.61.0",
    "node": "^16.5.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.ecsstractor.run"
  ],
  "main": "./out/vscode/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.ecsstractor.run",
        "title": "Run: eCSStractor"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "lint": "eslint src --ext ts",
    "watch": "tsc -watch -p ./",
    "test": "jest",
    "test_with_vscode": "node ./out/__tests__/runTest.js",
    "format": "prettier --write ./src/**/*.{js,ts}",
    "release": "semantic-release"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/preset-env": "^7.16.4",
    "@babel/preset-typescript": "^7.16.0",
    "@semantic-release/changelog": "6.0.1",
    "@semantic-release/commit-analyzer": "9.0.1",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/github": "8.0.2",
    "@semantic-release/release-notes-generator": "10.0.3",
    "@types/glob": "7.2.0",
    "@types/jest": "^27.0.3",
    "@types/mocha": "9.0.0",
    "@types/node": "16.11.9",
    "@types/prettier": "2.4.2",
    "@types/react": "^17.0.36",
    "@types/vscode": "1.62.0",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "4.33.0",
    "babel-jest": "^27.4.2",
    "classnames": "^2.3.1",
    "eslint": "7.32.0",
    "glob": "7.2.0",
    "jest": "^27.4.3",
    "mocha": "9.1.3",
    "semantic-release": "18.0.0",
    "semantic-release-vsce": "3.5.0",
    "ts-node": "^10.4.0",
    "vscode-test": "1.6.1"
  },
  "dependencies": {
    "@typescript-eslint/experimental-utils": "^4.31.1",
    "@typescript-eslint/typescript-estree": "^4.31.0",
    "domhandler": "^4.2.2",
    "htmlparser2": "^7.2.0",
    "option-t": "^29.1.0",
    "prettier": "^2.4.1",
    "typescript": "^4.5.2"
  }
}
