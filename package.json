{
  "name": "ecsstractor",
  "displayName": "eCSStractor",
  "description": "Extracting selectors from HTML / JSX / TSX and generate CSS file.",
  "version": "2.4.2",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/kubosho/vscode-ecsstractor.git"
  },
  "publisher": "kubosho",
  "engines": {
    "vscode": "^1.63.1",
    "node": "^16.5.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.ecsstractor.run"
  ],
  "main": "./out/vscode/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.ecsstractor.run",
        "title": "Run: eCSStractor"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "build": "vsce package",
    "lint": "eslint src --ext ts",
    "watch": "tsc -watch -p ./",
    "test": "jest",
    "format": "prettier --write ./src/**/*.{js,ts}",
    "release": "semantic-release"
  },
  "devDependencies": {
    "@babel/core": "^7.18.2",
    "@babel/preset-env": "^7.18.2",
    "@babel/preset-typescript": "^7.16.7",
    "@semantic-release/changelog": "6.0.1",
    "@semantic-release/commit-analyzer": "9.0.2",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/github": "8.0.2",
    "@semantic-release/release-notes-generator": "10.0.3",
    "@types/jest": "^27.4.0",
    "@types/node": "17.0.15",
    "@types/prettier": "2.6.1",
    "@types/react": "^17.0.39",
    "@types/vscode": "^1.68.0",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "4.33.0",
    "babel-jest": "^27.4.6",
    "classnames": "^2.3.1",
    "eslint": "7.32.0",
    "jest": "^27.4.7",
    "semantic-release": "19.0.3",
    "semantic-release-vsce": "5.0.6",
    "ts-node": "^10.4.0",
    "vsce": "^2.6.7"
  },
  "dependencies": {
    "@typescript-eslint/experimental-utils": "^5.10.2",
    "@typescript-eslint/typescript-estree": "^5.11.0",
    "domhandler": "^4.3.0",
    "htmlparser2": "^7.2.0",
    "option-t": "^30.0.0",
    "prettier": "^2.6.2",
    "typescript": "^4.5.5"
  }
}
